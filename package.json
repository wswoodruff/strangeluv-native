        "eslint-plugin-babel": "^4.1.1",
        "eslint-plugin-hapi": "^4.0.0",
        "eslint-plugin-react": "7.5.1",
        "eslint-plugin-react-native": "^3.1.0",
        "install": "^0.10.1",
        "npm": "^5.4.0",
