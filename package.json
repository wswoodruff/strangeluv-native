{
    "name": "strangeluvnative",
    "version": "0.0.1",
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "rc-start": "npm start -- --reset-cache",
        "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
        "clean-start": "npm run clean && npm run rc-start",
        "fresh-install": "rm -rf package-lock.json && rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm cache clean --force && rm -rf ios && rm -rf android && npm install react-native && npm install && react-native link",
        "fresh-start": "npm run fresh-install && npm run dev",
        "packager": "osascript -e 'tell application \"Terminal\" to do script \"cd '$(pwd)' && npm run rc-start\"'",
        "dev": "NODE_ENV=development npm run packager &&  react-native run-ios --simulator='iPhone 5s'",
        "i": "node node_modules/react-native/local-cli/cli.js run-ios",
        "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
        "a": "node node_modules/react-native/local-cli/cli.js run-android",
        "android": "node node_modules/react-native/local-cli/cli.js run-android --variant arDebug --config ../../../../rn-cli.config.js",
        "link": "node node_modules/react-native/local-cli/cli.js link",
        "remotedev": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
        "glog": "(cd node_modules/react-native/third-party/glog-0.3.4 && ../../scripts/ios-configure-glog.sh)",
        "postinstall": "npm run link && npm run remotedev && react-native upgrade && npm run viro-setup",
        "lint": "eslint src tests server",
        "lint:src": "eslint src",
        "rename": "node node_modules/react-native-rename/lib/index.js",
        "viro-setup": "node_modules/react-viro/bin/setup-ide.sh --all"
    },
    "pre-commit": [
        "lint"
    ],
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-plugin-root-import": "^5.1.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-react-native": "^4.0.0",
        "babel-runtime": "^6.20.0",
        "create-react-class": "^15.6.2",
        "debug": "^3.0.1",
        "fs-extra": "^6.0.1",
        "keymirror": "^0.1.1",
        "prop-types": "^15.6.0",
        "react": "16.4.0",
        "react-native": "^0.55.4",
        "react-navigation": "2.0.4",
        "react-redux": "5.0.7",
        "react-viro": "^2.8.1",
        "redux": "4.0.0",
        "redux-thunk": "2.3.0",
        "strange-motion": "^1.0.2",
        "styled-components": "^3.3.0"
    },
    "devDependencies": {
        "babel-eslint": "^8.0.0",
        "babel-jest": "23.0.1",
        "babel-plugin-globals": "^3.0.0",
        "babel-plugin-module-resolver": "^3.0.0",
        "babel-preset-react-native": "4.0.0",
        "eslint": "^4.6.1",
        "eslint-config-hapi": "^11.0.0",
        "eslint-config-standard-react": "^6.0.0",
        "eslint-plugin-babel": "^5.1.0",
        "eslint-plugin-hapi": "^4.0.0",
        "eslint-plugin-react": "7.8.2",
        "eslint-plugin-react-native": "^3.1.0",
        "pre-commit": "^1.2.2",
        "react-native-rename": "^2.2.2",
        "remote-redux-devtools": "^0.5.7",
        "remotedev-rn-debugger": "^0.8.3",
        "remotedev-server": "^0.2.4"
    }
}
